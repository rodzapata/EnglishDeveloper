// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider     = "prisma-client-js"
  output       = "../src/generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "sqlite"
}

model vocabulary {
  id            Int     @id @default(autoincrement())
  item          Int?
  description   String
  pronunciation String?
  translate     String?
  type          Int?    @default(2)
  day           Int?
}
